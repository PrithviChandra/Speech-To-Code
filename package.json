{
  "name": "speechtocode",
  "displayName": "speechtocode",
  "description": "Converts conversational natural language to code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:speechtocode.time_complexity",
    "onCommand:speechtocode.extensionMaker",
    "onCommand:speechtocode.bug_fixer",
    "onCommand:speechtocode.explain_code",
    "onCommand:speechtocode.translate_code"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "speechtocode.extensionMaker",
        "title": "Extension Maker"
      },
      {
        "command": "speechtocode.explain_code",
        "title": "Code Explanation"
      },
      {
        "command": "speechtocode.time_complexity",
        "title": "Time Complexity"
      },
      {
        "command": "speechtocode.bug_fixer",
        "title" : "Python Bug Fixer"
      },
      {
        "command": "speechtocode.translate_code",
        "title" : "Translate Code"
      }
    ],
    "keybindings": [
      {
        "command": "speechtocode.extensionMaker",
        "key": "ctrl+k ctrl+k",
        "mac": "cmd+k cmd+k"
      },
      {
        "command":"speechtocode.time_complexity",
        "key":"ctrl+l ctrl+l",
        "mac":"cmd+t cmd+t"
      },
      {
        "command":"speechtocode.bug_fixer",
        "key" : "ctrl+h ctrl+h",
        "mac" : "cmd+h cmd+h"
      },
      {
        "command":"speechtocode.explain_code",
        "key" : "ctrl+e ctrl+e",
        "mac" : "cmd+e cmd+e"
      },
      {
        "command":"speechtocode.translate_code",
        "key" : "ctrl+m ctrl+m",
        "mac" : "cmd+m cmd+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "openai": "^2.0.4"
  }
}
